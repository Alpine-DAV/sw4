# SW4 input file, testing mr+q
fileio path=mr-test-4th-c.sw4output verbose=1
#fileio path=mr-test-2nd-c.sw4output verbose=1

#developer time_order=2 cfl=0.8

# h=400/200/100 vmin: 500.0
grid x=30000.0 y=40000.0 z=35000.0 h=400.0 lat=-0.089832 lon=-0.179663 az=0.0 mlat=111319.5 mlon=111319.5

# MR, hbase=400.0 hmin=100.0
refinement zmax=5000
# need to move the 2nd refinement to get 12 points in top grid
refinement zmax=1100

time t=20.0
supergrid gp=30
attenuation phasefreq=1.0 nmech=3 maxfreq=1.25

